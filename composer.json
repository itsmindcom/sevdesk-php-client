{
    "name": "itsmind/sevdesk-php-client",
    "type": "library",
    "description": "This package was renamed to itsmind/sevdesk-php-sdk. Please use that package instead. To migrate, you only have to replace the dependency in your composer.json, everything else remains the same. An unofficial SDK for the official sevDesk API. Always up to date, powered by [sevdesk-api](https://github.com/j-mastr/sevdesk-api) and the OpenAPI Generator.",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "sevdesk",
        "accounting"
    ],
    "homepage": "https://api.sevdesk.de/",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        },
        {
            "name": "sevDesk",
            "homepage": "https://sevdesk.de/"
        },
        {
            "name": "it's mind",
            "homepage": "https://itsmind.com/"
        },
        {
            "name": "Janik Stracke",
            "homepage": "https://github.com/j-mastr"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Itsmind\\Sevdesk\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itsmind\\Sevdesk\\Test\\": "test/"
        }
    },
    "scripts": {
        "post-autoload-dump": "echo -e \"\\n\\n\\033[37;41m#################################################################\\n#\\t\\t\\t\\t\\t\\t\\t\\t#\\n#\\tThe package \\033[37;41;1mitsmind/sevdesk-php-client\\033[0m\\033[37;41m was renamed.\\t#\\n#\\tTo migrate, please change the dependency in your\\t#\\n#\\t\\033[37;41;3mcomposer.json\\033[0m\\033[37;41m file to \\033[32;49;1mitsmind/sevdesk-php-sdk\\033[0m\\033[37;41m.\\t#\\n#\\t\\t\\t\\t\\t\\t\\t\\t#\\n#################################################################\\033[0m\\n\\n\""
    }
}
